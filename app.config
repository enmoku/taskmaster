	<system.diagnostics>
		<trace autoflush="false" indentsize="4">
			<listeners>
				<add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener" traceOutputOptions="ProcessId, DateTime" />
				<add name="fileListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="Taskmaster.TextWriterOutput.log" />
				<remove name="Default" />
			</listeners>
		</trace>
	</system.diagnostics>
	-->
</configuration>
